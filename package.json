{
  "name": "dataply",
  "version": "0.0.16-alpha.6",
  "description": "A lightweight storage engine for Node.js with support for MVCC, WAL.",
  "license": "MIT",
  "author": "izure <admin@izure.org>",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest -i",
    "benchmark": "npx tsx benchmark/benchmark.ts",
    "build": "node build/index.js && tsc"
  },
  "keywords": [
    "storage-engine",
    "mvcc",
    "wal",
    "sharding",
    "database",
    "bplus-tree",
    "typescript",
    "record-storage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/izure1/dataply.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "esbuild": "^0.27.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "cache-entanglement": "^1.7.1",
    "hookall": "^2.2.0",
    "ryoiki": "^1.2.0",
    "serializable-bptree": "^7.0.2"
  }
}